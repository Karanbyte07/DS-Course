class Employee:
    company = "HP"
    def __init__(self, name, salary):
        self.name = name
        self.salary = salary
        
    def printDetails(self):
        print(f"The company of {self.name} is {self.company} with salary ${self.salary}")
        
    @staticmethod
    def printTime():
        print(f"The time is now")

    @classmethod
    def printClassDetails(cls):
        print(f"The company is {cls.company}")
        

e = Employee("Harry", 47000)
e.printDetails()
e.printTime()
e.printClassDetails()


# Define a class named Student
class Student:
    # Constructor method to initialize the student's name, roll number, and section
    def __init__(self, name, rollNo, section):
        self.name = name        # Store the student's name
        self.rollNo = rollNo    # Store the student's roll number
        self.section = section  # Store the student's section

    # Method to print the student's details in a formatted string
    def printDetails(self):
        print(f"The name of student is {self.name} in section {self.section} with rollNo {self.rollNo}")

# Create an object 'S1' of the Student class with specific details
S1 = Student("Karanjeet", 10, "55")
S2 = Student("amarjeet", 20, "S8")

# Call the printDetails method to display the student's information
S1.printDetails()
S2.printDetails()


class Car:
    def __init__(self, name, wheels, model ):
        self.name = name
        self.wheels = wheels;
        self.model = model
        

    def printCar(self):
